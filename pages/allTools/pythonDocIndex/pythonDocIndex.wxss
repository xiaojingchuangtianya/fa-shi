/* pages/allTools/pythonDocIndex/pythonDocIndex.wxss */
page,.container{
  width: 100%;
  height: 100%;
}
.head{
  width: 100%;
  height: 200px;
  background-color: aqua;
  text-align: center;
}

.oneSlide{
  
  width: 100%;
  height: 30px;
  background-color: rgb(140, 223, 198);
}
.Block{
  width: 40%;
  background-color: coral;
  height: 30px;
  border-radius: 30px;
  text-align: center;
  font-size: large;
  font-weight: bold;
}
.RslideTwo{
  animation: RslideTwo 0.5s linear forwards;
}
.RslideThree{
  animation: RslideThree 0.5s linear forwards;
}
.RslideOne{
  animation: RslideOne 0.5s linear forwards;
}

.LslideTwo{
  animation: LslideTwo 0.5s linear forwards;
}
.LslideThree{
  animation: LslideThree 0.5s linear forwards;
}
.LslideOne{
  animation: LslideOne 0.5s linear forwards;
}
.mainContent{
  height: calc(70% - 30px);
  width: 100%;
}
.mainContent , .showContent{
  width: 100%;
  height: calc(70% - 30px);
  display: flex;
  justify-content: space-around;
}
.mainContent  .one{
  position: absolute;
  left: 0;
  height: calc(70% - 30px);
  background-color: cornsilk;
}
.mainContent  .two{
  z-index: 3;
  position: absolute;
  left: 0;
  height: calc(70% - 30px);
  background-color: thistle;
}
.mainContent  .three{
  position: absolute;
  right: 0;
  height: calc(70% - 30px);
  background-color: tan;
}
/* 决定那个展示在最高层级 */
.mainContent .show{
  z-index:3;
}
.mainContent .hide{
  z-index: 0;
}
.mainContent .showContent .box{
  margin-top: 3px;
  width: 100px;
  height: 70px;
  background-color: rgb(222, 235, 227);
  /* outline: 2px solid whitesmoke;设置为与背景颜色一致 */
  box-shadow: 0 0 0 2px rgb(225, 228, 227);
}



